@using NewLife.Cube.Enums;
@{
    var page = ViewBag.Page as Pager;

    var enumList = typeof(PlateColor).GetAllEnums();

    var dic = new Dictionary<Int32, String>();
    foreach (var item in enumList)
    {
        dic.Add(item.Value, item.Name);
    }
}
<div class="form-group">
    <label for="enable" class="control-label">车牌颜色：</label>
    @await Html.PartialAsync("_SelectPlateColor", new ListBoxModel("PlateColor", dic,  page["PlateColor"], "全部", true, null))
</div>
